@import 'nib'

a, a:link
	cursor pointer

p.bigger
	font-size 120%

body
	color #2e7d0d
	background-color black
	padding-top 60px

div.container
	margin-left auto
	margin-right auto
	width 650px

div.jumbotron
	color black
	background-color #1d4f08

a.btn
	background-color #1d4f08

a:link
	color #d60d0d

a:visited
	color #a60707

footer
	padding-top 30px
	padding-bottom 30px

img
	padding-top 10px
	padding-bottom 10px

table
	tfoot, tbody
		th
			border-top 1px solid #DDD

h1
	padding-bottom 10px
